{
  "name": "tadaima",
  "version": "0.6.0",
  "description": "Stylish, pure CSS, user-friendly web design framework.",
  "style": "dist/",
  "main": "scss/tadaima.scss",
  "scripts": {
    "build": "npm run build-sass && npm run post-build",
    "build-sass": "node-sass --include-path --output-style expanded --source-map true --indent-type space --indent-width 2 --linefeed crlf scss/tadaima.scss dist/tadaima.css",
    "post-build": "postcss dist/tadaima.css --use autoprefixer cssnano -m true -o dist/tadaima.min.css",
    "watch": "npm-run-all -p \"build -- --watch\""
  },
  "author": {
    "name": "D8vjork",
    "email": "contact@d8vjork.es",
    "url": "https://d8vjork.es"
  },
  "contributors": [
    "Tadaima Contributors (https://github.com/Social-chan/Tadaima/graphs/contributors)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Social-chan/Tadaima.git"
  },
  "keywords": [
    "front-end",
    "design",
    "html",
    "css",
    "framework",
    "tadaima"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Social-chan/Tadaima/issues"
  },
  "homepage": "https://tadaima.social-chan.com",
  "peerDependencies": {
    "feather-icons": "^3.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "cpy-cli": "^1.0.1",
    "cssnano": "^3.10.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.0",
    "rimraf": "^2.6.1",
    "watch": "^1.0.2"
  },
  "files": [
    "scss",
    "dist",
    "src",
    "LICENSE"
  ]
}
